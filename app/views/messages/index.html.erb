<%= javascript_include_tag 'message' %>

<% @messages.each do |message| %>
  <%= message.message %>
  <%= message.created_at.strftime("%Y/%m/%d %H:%M") %>
  <%= message.id %>
<% end %>
<ul class="messages">
</ul>


<%= form_for @message, html: { class: 'form' } do |f|%>
  <%= f.text_field :message ,class: 'message_content'%>
  <%= f.submit class: 'submit' %>
<% end %>
